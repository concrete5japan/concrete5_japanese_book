<?xml version="1.0"?>
<schema version="0.3">
	<table name="btDateNav">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
		<field name="num" type="I2">
			<unsigned />
			<notnull />
		</field> 
		
		<field name="cParentID" type="I">
			<unsigned />
			<notnull />
			<default value="1" />
		</field>
		
		<field name="cThis" type="I1">
			<unsigned />
			<notnull />
			<default value="0" />
		</field> 
		
		<field name="ctID" type="I2">
			<unsigned />
		</field> 
		
		<field name="flatDisplay" type="I">
			<default value="0"/>
		</field>	
		
		<field name="defaultNode" type="C" size="64">
			<default value="current_page"/>
		</field>	
					
		<field name="truncateTitles" type="I">
			<default value="0"/>
		</field>
		
		<field name="truncateSummaries" type="I">
			<default value="0"/>
		</field>
		
		<field name="displayFeaturedOnly" type="I">
			<default value="0"/>
		</field>
		<field name="truncateChars" type="I">
			<default value="128"/>
		</field>
		<field name="truncateTitleChars" type="I">
			<default value="128"/>
		</field>		
		<field name="showDescriptions" type="I">
			<default value="0"/> 
		</field>
				
	</table>
</schema>